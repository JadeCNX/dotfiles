{
  "coc.preferences.colorSupport": true,
  /* "coc.preferences.extensionUpdateCheck": "never", */
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  /* "codeLens.enable": true, */
  "cSpell.enabled": false,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.showCommandsInEditorContextMenu": false,
  "git.addGBlameToVirtualText": true,
  "git.addGBlameToBufferVar": false,
  "git.enableGutters": false,
  "json.enable": true,
  "list.indicator": ">",
  "list.nextKeymap": "<c-n>",
  "list.previousKeymap": "<c-p>",
  "python.pythonPath": "python3",
  "signature.target": "echo",
  "suggest.autoTrigger": "always",
  "suggest.echodocSupport": true,
  "suggest.enablePreview": true,
  "suggest.noselect": true,
  "suggest.snippetIndicator": "⚡️",
  "suggest.triggerAfterInsertEnter": true,
  "explorer.icon.enableNerdfont": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "clangd.semanticHighlighting": true,
  "snippets.ultisnips.directories": ["UltiSnips", "gosnippets/UltiSnips"],
  "snippets.autoTrigger": true,
  "snippets.enableStatusItem": true,
  "tsserver.enable": true,
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "tailwindCSS.trace.server": "verbose",
  "yank.enableCompletion": true,
  /* "coc.preferences.hoverTarget": "echo", */
  /* "suggest.floatEnable": false, */
  /* "diagnostic.virtualText": true, */
  /* "diagnostic.checkCurrentLine": true, */
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableSign": true,
  /* "diagnostic.messageTarget": "echo", */
  "diagnostic.refreshOnInsertMode": true,
  "html.autoClosingTags": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
