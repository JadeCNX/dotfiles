{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.enabled": false,
  "diagnostic.enableSign": true,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.refreshOnInsertMode": true,
  "explorer.file.reveal.auto": true,
  "explorer.icon.enableNerdfont": true,
  "git.addGBlameToBufferVar": false,
  "git.addGBlameToVirtualText": true,
  "git.enableGutters": false,
  "json.enable": true,
  "tailwindCSS.headwind.removeDuplicates": true,
  "tailwindCSS.headwind.runOnSave": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "python.pythonPath": "python3",
  "signature.target": "float",
  "snippets.autoTrigger": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.ultisnips.directories": ["UltiSnips", "gosnippets/UltiSnips"],
  "suggest.triggerAfterInsertEnter": true,
  "cSpell.userWords": ["JWKS", "Unmount", "resizer", "semibold", "toastify"]
}
