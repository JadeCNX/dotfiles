
snippet fori "for with key" b
for (let ${1:i} in ${2:items}) {
	let ${3:item} = $2[$1];
	$4
}
endsnippet

snippet for "for loop" b
for (let ${1:i} = 0; $1 < $2; $1++) {
	$4
}
endsnippet

snippet forl "for with lenth" b
for (let ${1:i} = 0, len = ${2:list}.length; $1 < len; $1++) {
	let ${3:item} = $2[$1];
	$4
}
endsnippet

snippet < "<tag></tag>" i
<${1:div}>$2</$1>
endsnippet

